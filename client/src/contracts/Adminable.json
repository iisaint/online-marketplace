  "source": "pragma solidity ^0.5.0;\n\ncontract Adminable {\n  address payable public admin;\n\n  modifier onlyAdmin() {\n    require(msg.sender == admin, \"caller is not admin.\");\n    _;\n  }\n\n  constructor() public {\n    admin = msg.sender;\n  }\n\n}",
